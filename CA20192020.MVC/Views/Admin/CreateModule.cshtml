@model IEnumerable<CA20192020.DataModel.Classes.Module>


<h3>Create new module or edit the existing ones.</h3>
<p>
    @Html.ActionLink("Create New", "CreateNewModule", "Admin")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ModuleName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModuleCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ModuleName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModuleCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
    </tr>
}
@if(ViewBag.Success == true)
{
    Html.Label("Module created successfully!");
}
</table>
